{
  "expo": {
    "name": "FrontierAtlas",
    "slug": "FrontierAtlas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/startup/FrontierAtlasLogo_ver1.png",
    "scheme": "frontieratlas",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/startup/FrontierAtlasLogo_ver1.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/startup/FrontierAtlasLogo_ver1.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.htlost.FrontierAtlas"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/startup/FrontierAtlasLogo_ver1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/startup/FrontierAtlasLogo_ver1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1b6e78b6-c7c3-4760-b085-bb85043c0650"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1b6e78b6-c7c3-4760-b085-bb85043c0650"
    }
  }
}
